{
  "name": "qiankun-vue2.0",
  "version": "1.0.0",
  "description": "基于qiankun+vue2.0技术栈实现的前端微应用架构。",
  "main": "index.js",
  "scripts": {
    "install": "npm-run-all install:* ",
    "install:main": "cd main && pnpm install ",
    "install:sub01": "cd sub01 && pnpm install ",
    "install:sub02": "cd sub02 && pnpm install ",
    "install:sub03": "cd sub03 && pnpm install ",
    "start": "npm-run-all start:* ",
    "start:main": "cd main && start cmd /k pnpm run dev ",
    "start:sub01": "cd sub01 && start cmd /k pnpm run dev  ",
    "start:sub02": "cd sub02 && start cmd /k pnpm run dev  ",
    "start:sub03": "cd sub03 && start cmd /k pnpm run dev  ",
    "update": "npm-run-all update:* ",
    "update:main": "cd main && pnpm install qiankun-vue2-common@1.4.1 ",
    "update:sub01": "cd sub01 && pnpm install qiankun-vue2-common@1.4.1 ",
    "update:sub02": "cd sub02 && pnpm install qiankun-vue2-common@1.4.1 ",
    "lint": "npm-run-all lint:* ",
    "lint:main": "cd main && pnpm run lint ",
    "lint:sub01": "cd sub01 && pnpm run lint ",
    "lint:sub02": "cd sub02 && pnpm run lint ",
    "build": "npm-run-all build:* ",
    "build:main": "cd main && pnpm run build ",
    "build:sub01": "cd sub01 && pnpm run build ",
    "build:sub02": "cd sub02 && pnpm run build "
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/lizhijie429/qiankun-vue2.0.git"
  },
  "keywords": [
    "qiankun",
    "vue2.0",
    "前端微应用"
  ],
  "author": "lizhijie429",
  "license": "MIT",
  "devDependencies": {
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "qs": "^6.10.1"
  }
}
