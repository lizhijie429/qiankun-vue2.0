{
  "name": "qiankun-vue2.0",
  "version": "1.0.0",
  "description": "基于qiankun+vue2.0技术栈实现的前端微应用架构。",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "npm-run-all install:* ",
    "install:main": "cd main && npm install ",
    "install:sub01": "cd sub01 && npm install ",
    "install:sub02": "cd sub02 && npm install ",
    "start": "npm-run-all start:* ",
    "start:main": "cd main && start cmd /k npm run dev ",
    "start:sub01": "cd sub01 && start cmd /k npm run dev  ",
    "start:sub02": "cd sub02 && start cmd /k npm run dev  ",
    "update": "npm-run-all update:* ",
    "update:main": "cd main && npm install qiankun-vue2-common@1.3.4 ",
    "update:sub01": "cd sub01 && npm install qiankun-vue2-common@1.3.4 ",
    "update:sub02": "cd sub02 && npm install qiankun-vue2-common@1.3.4 ",
    "lint": "npm-run-all lint:* ",
    "lint:main": "cd main && npm run lint ",
    "lint:sub01": "cd sub01 && npm run lint ",
    "lint:sub02": "cd sub02 && npm run lint ",
    "build": "npm-run-all build:* ",
    "build:main": "cd main && npm run build ",
    "build:sub01": "cd sub01 && npm run build ",
    "build:sub02": "cd sub02 && npm run build "
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/lizhijie429/qiankun-vue2.0.git"
  },
  "keywords": [
    "qiankun",
    "vue2.0",
    "前端微应用"
  ],
  "author": "lizhijie429",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "qs": "^6.10.1"
  }
}
